# Create 3 branches: public 1, public 2 and private
git checkout -b "public1"
git checkout -b "public2"
git checkout -b "private"

# Put public1.txt on public 1 branch, stage and commit
git checkout public1
touch public1.txt
git add public1.txt
git commit -m "Add public1.txt to public1 branch"

# Merge public 1 on master branch (We can also merge on github GUI)
git checkout master
git merge public1 -m "Merge public1 into master"

# Merge public 2 on master branch (We can also merge on github GUI)
git checkout public2
touch public2.txt
git add public2.txt
git commit -m "Add public2.txt to public2 branch"

git checkout master
git merge public2 -m "Merge public2 into master"

# Edit master.txt on private branch, stage and commit
git checkout private
nanop master.txt
git add master.txt
git commit -m "Edit in master.txt in private branch"

# Now update branch public 1 and public 2 with new master code in private
git checkout public1
git merge master -m "Update public1 with new master changes"

git checkout public2
git merge master -m "Update public2 with new master changes"

# Also update new master code on master
git checkout master
git merge private -m "Merge new changes from private to master"

# Finally update all the code on the private branch
git checkout private
git merge master -m "Update private with new master code"
     


